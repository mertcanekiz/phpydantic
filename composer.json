{
  "name": "mertcanekiz/phpydantic",
  "description": "A reflection-driven BaseModel inspired by pydantic for JSON Schema generation in PHP",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Phpydantic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phpydantic\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1"
  },
  "minimum-stability": "stable",
  "scripts": {
    "test": "phpunit --colors tests/"
  }
}
